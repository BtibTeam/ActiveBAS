<?xml version="1.0" encoding="UTF-8"?>
<!-- Niagara Presentation XML -->
<px version="1.0">
<import>
  <module name="baja"/>
  <module name="bajaui"/>
  <module name="converters"/>
  <module name="gx"/>
</import>
<properties>

  <property name="nomSite" type="baja:String" value="Le rabelais">
  </property>

  <property name="gaucheForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="gaucheBackgroundCouleurClaire" type="gx:Brush" value="#616161">
  </property>

  <property name="gaucheBackgroundCouleur" type="gx:Brush" value="#424242">
  </property>

  <property name="gaucheBackgroundCouleurSombre" type="gx:Brush" value="#212121">
  </property>

  <property name="enTeteForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="enTeteBackgroundCouleurClaire" type="gx:Brush" value="#039be5">
  </property>

  <property name="enTeteBackgroundCouleur" type="gx:Brush" value="#0288d1">
  </property>

  <property name="enTeteBackgroundCouleurSombre" type="gx:Brush" value="#0277bd">
  </property>

  <property name="logoWhite" type="gx:Image" value="file:^images/logos/logo_w.svg">
  </property>

  <property name="vueCentraleForegroundNoire" type="gx:Brush" value="black">
  </property>

  <property name="vueCentraleBackgroundBlanc" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleBackgroundCouleurClaire" type="gx:Brush" value="#546e7a">
  </property>

  <property name="vueCentraleBackgroundCouleur" type="gx:Brush" value="#455a64">
  </property>

  <property name="vueCentraleBackgroundCouleurSombre" type="gx:Brush" value="#37474f">
  </property>

  <property name="vueCentraleElementForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleElementBackgroundCouleurClaire" type="gx:Brush" value="#1e88e5">
  </property>

  <property name="vueCentraleElementBackgroundCouleur" type="gx:Brush" value="#1976d2">
  </property>

  <property name="vueCentraleElementBackgroundCouleurSombre" type="gx:Brush" value="#1565c0">
  </property>

</properties>
<content>
<ScrollPane>

  <CanvasPane name="content" viewSize="1000.0,800.0" minScaleFactor="0.5" maxScaleFactor="1.0" background="image( source(file:^images/arriere_plans/netatmoWeatherCoach.png) )">

    <Line stroke="white" geom="455.0,345.0 235.0,265.0"/>

    <Label layout="90.0,270.0,140.0,60.0" font="36.0pt Pristina" foreground="#161616">
      <ValueBinding ord="slot:StationData/points/CO2">
        <ObjectToString name="text" format="%out.value%"/>
      </ValueBinding>
    </Label>

    <Picture layout="110.0,170.0,100.0,100.0" image="file:^images/icones/co2.png" scale="fitRatio"/>

    <Picture layout="130.0,490.0,100.0,100.0" image="file:^images/icones/humidite.png" scale="fitRatio"/>

    <Label layout="120.0,600.0,120.0,50.0" font="36.0pt Pristina" foreground="#161616">
      <ValueBinding ord="slot:StationData/points/Humidity">
        <ObjectToString name="text" format="%out.value%"/>
      </ValueBinding>
    </Label>

    <Line stroke="white" geom="455.0,445.0 245.0,545.0"/>

    <Picture layout="810.0,140.0,100.0,100.0" image="file:^images/icones/temperature.png" scale="fitRatio"/>

    <Label layout="800.0,250.0,120.0,50.0" font="36.0pt Pristina" foreground="#161616">
      <ValueBinding ord="slot:StationData/points/Temperature">
        <ObjectToString name="text" format="%out.value%"/>
      </ValueBinding>
    </Label>

    <Line stroke="white" geom="800.0,245.0 600.0,395.0"/>

    <Line stroke="white" geom="810.0,520.0 600.0,460.0"/>

    <Picture layout="820.0,460.0,100.0,100.0" image="file:^images/icones/protegeOreille.png" scale="fitRatio"/>

    <Label layout="810.0,570.0,120.0,50.0" font="36.0pt Pristina" foreground="#161616">
      <ValueBinding ord="slot:StationData/points/Noise">
        <ObjectToString name="text" format="%out.value%"/>
      </ValueBinding>
    </Label>

  </CanvasPane>

</ScrollPane>
</content>
</px>
