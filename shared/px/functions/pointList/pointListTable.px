<content>
<ScrollPane>

  <EdgePane name="content">

    <PxInclude name="top" ord="file:^px/menus/haut.px"/>

    <PxInclude name="left" ord="file:^px/menus/gauche.px"/>

    <WebWidget name="center" js="view:btibVision:EnhancedBoundTable" preferredSize="1300.0,800.0">
      <WbViewBinding ord="slot:BqlSize"/>
      <Ord name="query" f="3" value="station:|slot:/Drivers|bql:select displayName as &#x27;Nom affichage&#x27;, proxyExt.device.displayName as &#x27;Nom equipement&#x27; from control:ControlPoint where (status.isOverridden = &#x27;true&#x27;)"/>
      <Ord name="hyperlink" f="3" value="slot:"/>
      <Double name="rowCount" f="3" value="500.0"/>
      <Boolean name="showRowCountSelector" f="3" value="true"/>
      <Boolean name="showSearching" f="3" value="true"/>
      <Boolean name="showColumnSelector" f="3" value="true"/>
      <Boolean name="showInfo" f="3" value="true"/>
      <Boolean name="showExporting" f="3" value="true"/>
      <ValueBinding ord="slot:BqlSize/executeQuery">
        <PassThrough name="query"/>
      </ValueBinding>
    </WebWidget>

    <PxInclude name="right" ord="file:^px/menus/droite.px"/>

  </EdgePane>

